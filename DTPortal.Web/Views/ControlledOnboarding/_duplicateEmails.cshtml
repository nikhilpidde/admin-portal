@model DTPortal.Web.ViewModel.ControlledOnboarding.DuplicateEmailViewModel


<div class="modal-dialog modal-dialog-centered modal-lg" role="document" style="width:500px">
    <div class="modal-content">
        <div class="modal-header">
            <h5 class="modal-title s3">
                <span class="text-capitalize s3" style="font-size: 18px">
                    Duplicate Emails
                </span>
            </h5>
            <button type="button"
                    class="close cross"
                    data-dismiss="modal">
                &times;
            </button>
        </div>
        <div style="margin-left:3px"><span><strong>Note:</strong> Please remove duplicate emails and upload file again</span></div>
        <div class="modal-body" style="min-height: 200px; max-height: 260px; overflow: auto;">
            <div class="well">
                <div class="table-wrap">
                    <div class="table-responsive">
                        <table class="table display product-overview mb-30" id="support_table5">
                                <tbody>
                                @if (Model.EmailList.Count > 0)
                                {
                                    @foreach (var email in Model.EmailList)
                                    {
                                        <tr>
                                            <td style="text-align:center">@email</td>
                                        </tr>
                                    }
                                }
                                </tbody>
                                    
                        </table>
                    </div>
                </div>
            </div>
        </div>
        <div class="modal-footer">
            <button class="btn btn-secondary" data-dismiss="modal">Close</button>
        </div>
    </div>
</div>


